<?xml version="1.0" encoding="utf-8"?>
<vn.icheck.android.ichecklibs.view.app_background.white_background.ICLinearLayoutWhite xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <vn.icheck.android.ichecklibs.view.secondary.TextSecondary
        android:id="@+id/tv_dn_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_12"
        android:layout_marginLeft="@dimen/size_12"
        android:layout_marginTop="@dimen/size_20"
        android:fontFamily="@font/roboto_regular"
        android:text="Doanh nghiệp sở hữu"
        android:textSize="@dimen/font_18" />

    <androidx.cardview.widget.CardView
        android:id="@+id/root"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_12"
        android:layout_marginTop="@dimen/size_8"
        android:layout_marginEnd="@dimen/size_12"
        android:layout_marginBottom="@dimen/size_12"
        app:cardCornerRadius="@dimen/size_10">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/gra1y_item_search">

            <LinearLayout
                android:id="@+id/owner_name_vg"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:background="@drawable/bg_corner_top_dnsh_thinh_long_10">

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_owner_name"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/size_12"
                    android:layout_marginLeft="@dimen/size_12"
                    android:layout_marginTop="@dimen/size_7"
                    android:layout_marginEnd="@dimen/size_14"
                    android:layout_marginRight="@dimen/size_14"
                    android:layout_marginBottom="@dimen/size_6"
                    android:layout_weight="1"
                    android:fontFamily="@font/roboto_medium"
                    android:textColor="@color/white"
                    android:textSize="@dimen/font_16"
                    tools:text="Công ty CP Thương mại quốc tế Thịnh Long" />

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/img_not_verified"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/ic_evs_verified" />

                <androidx.appcompat.widget.AppCompatImageButton
                    android:id="@+id/btn_owner_next"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginEnd="@dimen/size_8"
                    android:layout_marginRight="@dimen/size_8"
                    android:background="@null"
                    app:srcCompat="@drawable/ic_next_white_18px" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/size_8"
                android:layout_marginBottom="@dimen/size_10"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/owner_name_vg">

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_owner_mst"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/size_12"
                    android:layout_marginEnd="@dimen/size_12"
                    android:fontFamily="@font/roboto_medium"
                    android:textColor="#000000"
                    tools:text="MST: 0312983404" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_owner_address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/size_12"
                    android:layout_marginTop="@dimen/size_4"
                    android:layout_marginEnd="@dimen/size_12"
                    android:fontFamily="@font/roboto_regular"
                    android:maxLines="2"
                    android:textColor="#000000"
                    tools:text="149/30 Bành Văn Trân, Phường 7, Quận Tân Bình, TP Hồ Chí Minh" />

                <vn.icheck.android.ichecklibs.view.secondary.TextSecondary
                    android:id="@+id/tv_owner_mail"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/size_12"
                    android:layout_marginEnd="@dimen/size_12"
                    android:drawableStart="@drawable/ic_web_dnsh_thinh_long"
                    android:drawableLeft="@drawable/ic_web_dnsh_thinh_long"
                    android:drawablePadding="8.5dp"
                    android:ellipsize="end"
                    android:fontFamily="@font/roboto_regular"
                    android:gravity="center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:paddingStart="@dimen/size_1"
                    android:paddingTop="@dimen/size_4"
                    tools:text="www.mediwhite.vn" />

                <vn.icheck.android.ichecklibs.view.secondary.TextSecondary
                    android:id="@+id/tv_owner_phone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/size_12"
                    android:layout_marginLeft="@dimen/size_12"
                    android:layout_marginEnd="@dimen/size_12"
                    android:drawableStart="@drawable/ic_phone_blue_18px"
                    android:drawableLeft="@drawable/ic_phone_blue_18px"
                    android:drawablePadding="@dimen/size_6"
                    android:fontFamily="@font/roboto_regular"
                    android:maxLines="1"
                    android:paddingTop="@dimen/size_4"
                    tools:text="0985771133" />

                <vn.icheck.android.ichecklibs.view.secondary.TextSecondary
                    android:id="@+id/tv_owner_website"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/size_12"
                    android:layout_marginEnd="@dimen/size_12"
                    android:drawableStart="@drawable/ic_website_blue_18px"
                    android:drawableLeft="@drawable/ic_website_blue_18px"
                    android:drawablePadding="@dimen/size_6"
                    android:fontFamily="@font/roboto_regular"
                    android:maxLines="1"
                    android:paddingTop="@dimen/size_4"
                    tools:text="cskh@mediwhite.vn" />
            </LinearLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</vn.icheck.android.ichecklibs.view.app_background.white_background.ICLinearLayoutWhite>