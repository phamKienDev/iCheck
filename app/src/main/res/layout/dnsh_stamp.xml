<?xml version="1.0" encoding="utf-8"?>
<vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICLinearLayoutWhite xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <vn.icheck.android.ichecklibs.view.secondary.TextSecondary
        android:id="@+id/tv_dn_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_12"
        android:layout_marginTop="@dimen/size_20"
        android:fontFamily="@font/roboto_regular"
        android:textSize="@dimen/font_18"
        android:text="Doanh nghiệp sở hữu" />

    <androidx.cardview.widget.CardView
        android:id="@+id/root"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_12"
        android:layout_marginTop="@dimen/size_8"
        android:layout_marginEnd="@dimen/size_12"
        android:layout_marginBottom="@dimen/size_12"
        app:cardCornerRadius="@dimen/size_10">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/gra1y_item_search">

            <LinearLayout
                android:id="@+id/owner_name_vg"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_owner_name"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/size_12"
                    android:layout_marginTop="@dimen/size_7"
                    android:layout_marginBottom="@dimen/size_6"
                    android:layout_weight="1"
                    android:ellipsize="end"
                    android:fontFamily="@font/roboto_medium"
                    android:maxLines="1"
                    android:textColor="@color/white"
                    android:textSize="@dimen/font_16"
                    tools:text="Công ty Cổ phần Nano Techco" />

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/img_not_verified"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/ic_not_verified_red_24px" />

                <androidx.appcompat.widget.AppCompatImageButton
                    android:id="@+id/btn_owner_next"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginEnd="@dimen/size_8"
                    android:layout_marginRight="@dimen/size_8"
                    android:background="@null"
                    app:srcCompat="@drawable/ic_next_white_18px" />
            </LinearLayout>

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_owner_mst"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/size_12"
                android:layout_marginTop="@dimen/size_8"
                android:layout_marginEnd="@dimen/size_12"
                android:fontFamily="@font/roboto_medium"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/owner_name_vg"
                tools:text="MST: 0312983404" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_owner_address"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/size_12"
                android:layout_marginTop="@dimen/size_4"
                android:layout_marginEnd="@dimen/size_12"
                android:fontFamily="@font/roboto_regular"
                android:maxLines="2"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_owner_mst"
                tools:text="149/30 Bành Văn Trân, Phường 7, Quận Tân Bình, TP Hồ Chí Minh" />

            <vn.icheck.android.ichecklibs.view.secondary.TextSecondary
                android:id="@+id/tv_owner_website"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/size_12"
                android:layout_marginTop="@dimen/size_4"
                android:layout_marginEnd="@dimen/size_12"
                android:ellipsize="end"
                android:fontFamily="@font/roboto_regular"
                android:maxLines="1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_owner_address"
                tools:text="www.mediwhite.vn" />

            <vn.icheck.android.ichecklibs.view.secondary.TextSecondary
                android:id="@+id/tv_owner_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/size_12"
                android:layout_marginLeft="@dimen/size_12"
                android:layout_marginTop="@dimen/size_4"
                android:layout_marginBottom="@dimen/size_12"
                android:fontFamily="@font/roboto_regular"
                android:maxLines="1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_owner_website"
                tools:text="0985771133" />

            <vn.icheck.android.ichecklibs.view.secondary.TextSecondary
                android:id="@+id/tv_owner_mail"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/size_12"
                android:layout_marginLeft="@dimen/size_12"
                android:layout_marginEnd="@dimen/size_12"
                android:layout_marginRight="@dimen/size_12"
                android:fontFamily="@font/roboto_regular"
                android:gravity="end"
                android:maxLines="1"
                app:layout_constraintBottom_toBottomOf="@+id/tv_owner_phone"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/tv_owner_phone"
                app:layout_constraintTop_toTopOf="@+id/tv_owner_phone"
                tools:text="cskh@mediwhite.vn" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICLinearLayoutWhite>