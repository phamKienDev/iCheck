<?xml version="1.0" encoding="utf-8"?>
<vn.icheck.android.ichecklibs.view.appbackground.white_background.ICConstraintLayoutWhite xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    android:focusable="true">

    <include
        android:id="@id/layoutToolbar"
        layout="@layout/toolbar_black" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@id/recyclerView"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_0"
        android:layout_marginBottom="@dimen/size_10"
        app:layout_constraintBottom_toTopOf="@id/btnSkip"
        app:layout_constraintTop_toBottomOf="@id/layoutToolbar" />

    <View
        style="@style/Shadow_Bottom"
        android:layout_width="match_parent"
        app:layout_constraintTop_toBottomOf="@id/layoutToolbar" />

    <vn.icheck.android.ichecklibs.view.secondary.ButtonSecondary
        android:id="@id/btnSkip"
        style="@style/Button_White_Corners_6"
        android:layout_width="@dimen/size_0"
        android:layout_height="@dimen/size_36"
        android:layout_marginStart="@dimen/size_24"
        android:layout_marginEnd="@dimen/size_6"
        android:layout_marginBottom="@dimen/size_16"
        android:text="@string/bo_qua"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/btnSend"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@id/btnSend"
        style="@style/Button_Blue_Corners_6"
        android:layout_width="@dimen/size_0"
        android:layout_marginStart="@dimen/size_6"
        android:layout_marginEnd="@dimen/size_24"
        android:text="@string/gui"
        app:layout_constraintBottom_toBottomOf="@id/btnSkip"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/btnSkip"
        app:layout_constraintTop_toTopOf="@id/btnSkip" />

</vn.icheck.android.ichecklibs.view.appbackground.white_background.ICConstraintLayoutWhite>