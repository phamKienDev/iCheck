<?xml version="1.0" encoding="utf-8"?>
<vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICConstraintLayoutWhite xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/size_96"
    android:elevation="@dimen/size_4">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imgCloseDrawer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingStart="@dimen/size_12"
        android:paddingTop="@dimen/size_10"
        android:paddingEnd="@dimen/size_12"
        android:paddingBottom="@dimen/size_10"
        android:src="@drawable/ic_cancel_blue_24px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <vn.icheck.android.ichecklibs.view.primary.TextHeaderPrimary
        android:id="@+id/appCompatTextView44"
        tools:layout_editor_absoluteX="329dp"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="Bộ lọc"
        app:layout_constraintBottom_toBottomOf="@+id/imgCloseDrawer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgCloseDrawer" />

    <vn.icheck.android.ichecklibs.view.second_text.TextSecondBarlowRegular
        android:id="@+id/btnUncheckAll"
        android:text="Bỏ chọn"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:gravity="center"
        android:paddingStart="@dimen/size_12"
        android:paddingEnd="@dimen/size_12"
        android:textSize="@dimen/font_18"
        app:layout_constraintBottom_toBottomOf="@id/appCompatTextView44"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/appCompatTextView44" />

</vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICConstraintLayoutWhite>
