<?xml version="1.0" encoding="utf-8"?>
<vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICLinearLayoutWhite xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingTop="@dimen/size_16"
        android:paddingBottom="@dimen/size_16">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <vn.icheck.android.ichecklibs.view.secondary.TextHeaderSecondary
                android:id="@+id/tv_count_like"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textview" />

            <vn.icheck.android.ichecklibs.view.secondary.TextBarlowMediumSecondary
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/luot_thich_bai_viet"
                android:textSize="@dimen/font_14" />
        </LinearLayout>

        <vn.icheck.android.ichecklibs.view.line_color.ICViewLineColor
            android:layout_width="@dimen/size_1"
            android:layout_height="@dimen/size_20"/>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <vn.icheck.android.ichecklibs.view.secondary.TextHeaderSecondary
                android:id="@+id/tv_count_follow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textview" />

            <vn.icheck.android.ichecklibs.view.secondary.TextBarlowMediumSecondary
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/so_nguoi_theo_doi"
                android:textSize="@dimen/font_14" />
        </LinearLayout>
    </LinearLayout>

    <vn.icheck.android.ichecklibs.view.line_color.ICViewLineColor
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:layout_marginTop="@dimen/size_20">

        <vn.icheck.android.component.view.ListAvatar
            android:id="@+id/img_list_avatar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal" />
    </LinearLayout>

    <vn.icheck.android.ichecklibs.view.normal_text.TextNormalBarlowSemiBold
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/size_16"
        android:includeFontPadding="false"
        android:text="@string/moi_ban_be_theo_doi_trang_nay"
        android:textSize="@dimen/font_16" />

    <vn.icheck.android.ichecklibs.view.second_text.TextSecondBarlowMedium
        android:id="@+id/textView73"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/size_24"
        android:layout_marginTop="@dimen/size_5"
        android:layout_marginRight="@dimen/size_24"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="@string/moi_them_ban_be_de_cung_cap_nhap_cac_thong_tin_moi_nhat_tu_trang_nay"
        android:textSize="@dimen/font_14" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tvInvite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/size_22"
        android:drawableStart="@drawable/ic_user_follow_page_18dp"
        android:drawablePadding="@dimen/size_8"
        android:fontFamily="@font/barlow_semi_bold"
        android:paddingStart="@dimen/size_24"
        android:paddingLeft="@dimen/size_24"
        android:paddingTop="@dimen/size_10"
        android:paddingRight="@dimen/size_24"
        android:paddingBottom="@dimen/size_10"
        android:layout_marginBottom="@dimen/size_22"
        android:text="@string/moi_ban_be"
        android:textColor="@color/white"
        android:textSize="@dimen/font_16" />

    <vn.icheck.android.ichecklibs.view.appbackground.graybackground.ICViewGray
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_10"/>


</vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICLinearLayoutWhite>