<?xml version="1.0" encoding="utf-8"?>
<vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICLinearLayoutWhite xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/size_5"
    android:layout_marginBottom="@dimen/size_5"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/layoutImg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/size_12"
            android:layout_marginTop="@dimen/size_8"
            android:orientation="vertical"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/imgTopup"
                android:layout_width="@dimen/size_50"
                android:layout_height="@dimen/size_50"
                android:padding="@dimen/size_5" />

        </LinearLayout>

        <vn.icheck.android.ichecklibs.view.normal_text.TextNormalBarlowSemiBold
            android:id="@+id/tvNameNetwork"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/size_8"
            android:textSize="@dimen/font_14"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/layoutImg"
            app:layout_constraintTop_toTopOf="@+id/layoutImg"
            tools:text="Thẻ Mobifone" />

        <vn.icheck.android.ichecklibs.view.secondary.TextBarlowSemiBoldSecondary
            android:id="@+id/tvPriceTopup"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/size_8"
            android:textSize="@dimen/font_16"
            android:textStyle="normal"
            app:layout_constraintBottom_toBottomOf="@+id/tvDate"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tvNameNetwork"
            tools:text="50.000đ" />

        <vn.icheck.android.ichecklibs.view.second_text.TextSecondBarlowMedium
            android:id="@+id/tvDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/size_4"
            android:includeFontPadding="false"
            android:textSize="@dimen/font_12"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@+id/tvNameNetwork"
            app:layout_constraintTop_toBottomOf="@+id/tvNameNetwork"
            tools:text="12:52, 05/05/2019" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@id/line"
            android:layout_width="0dp"
            android:layout_height="2.5dp"
            android:layout_marginTop="@dimen/size_9"
            android:layout_marginEnd="@dimen/size_12"
            android:layerType="software"
            android:src="@drawable/line_dotted"
            app:layout_constraintBottom_toBottomOf="@id/layoutImg"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/tvDate"
            app:layout_constraintTop_toBottomOf="@+id/tvDate" />

        <vn.icheck.android.ichecklibs.view.second_text.TextSecondBarlowMedium
            android:id="@+id/tvSubMaThe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/size_10"
            android:text="Mã thẻ:"
            android:textSize="@dimen/font_14"
            app:layout_constraintStart_toStartOf="@id/line"
            app:layout_constraintTop_toBottomOf="@id/line" />

        <vn.icheck.android.ichecklibs.view.normal_text.TextNormalBarlowSemiBold
            android:id="@+id/tvMaThe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/font_14"
            app:layout_constraintEnd_toEndOf="@id/line"
            app:layout_constraintTop_toTopOf="@+id/tvSubMaThe"
            tools:text="04520440964420" />

        <vn.icheck.android.ichecklibs.view.second_text.TextSecondBarlowMedium
            android:id="@+id/tvSubSoSeri"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/size_4"
            android:layout_marginBottom="@dimen/size_8"
            android:text="Số seri:"
            android:textSize="@dimen/font_14"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@id/tvSubMaThe"
            app:layout_constraintTop_toBottomOf="@id/tvSubMaThe" />

        <vn.icheck.android.ichecklibs.view.normal_text.TextNormalBarlowSemiBold
            android:id="@+id/tvSeri"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/font_14"
            app:layout_constraintBottom_toBottomOf="@id/tvSubSoSeri"
            app:layout_constraintEnd_toEndOf="@id/tvMaThe"
            app:layout_constraintTop_toTopOf="@+id/tvSubSoSeri"
            tools:text="0499205202100" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <vn.icheck.android.ichecklibs.view.line_color.ICViewLineColor
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginStart="@dimen/size_9"
        android:layout_marginTop="15.5dp"
        android:layout_marginEnd="@dimen/size_9" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_36">

        <vn.icheck.android.ichecklibs.view.accent.accent_red.TextAccentRed
            android:id="@+id/tvSelectUsed"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/barlow_medium"
            android:gravity="center"
            android:text="Đánh dấu đã dùng"
            android:textSize="@dimen/font_14"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/view"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@id/view"
            android:layout_width="1.5dp"
            android:layout_height="@dimen/size_16"
            android:background="#cccccc"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@id/tvSelectUsed"
            app:layout_constraintEnd_toStartOf="@+id/tvLoadNow"
            app:layout_constraintStart_toEndOf="@id/tvSelectUsed"
            app:layout_constraintTop_toTopOf="@id/tvSelectUsed" />

        <vn.icheck.android.ichecklibs.view.primary.TextBarlowMediumPrimary
            android:id="@+id/tvLoadNow"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="Nạp ngay"
            android:textSize="@dimen/font_14"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@id/view"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/view"
            app:layout_constraintTop_toTopOf="@id/view" />

        <vn.icheck.android.ichecklibs.view.second_text.TextSecondBarlowSemiBold
            android:id="@+id/tvUsed"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@color/grayD8"
            android:gravity="center"
            android:paddingTop="@dimen/size_8"
            android:paddingBottom="@dimen/size_10"
            android:text="Mã thẻ đã dùng"
            android:textSize="@dimen/font_14"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICLinearLayoutWhite>