<?xml version="1.0" encoding="utf-8"?>
<vn.icheck.android.ichecklibs.view.appbackground.white_background.ICLinearLayoutWhite xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/size_10"
    android:orientation="vertical">

    <vn.icheck.android.ichecklibs.view.secondary.TextBarlowSemiBoldSecondary
        android:id="@id/tvTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/size_10"
        android:includeFontPadding="false"
        android:paddingStart="@dimen/size_10"
        android:paddingEnd="@dimen/size_0"
        android:text="@string/trang_lien_quan"
        android:textSize="@dimen/font_16" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@id/recyclerView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clipToPadding="false"
        android:orientation="horizontal"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        tools:itemCount="3"
        tools:listitem="@layout/item_related_page" />
</vn.icheck.android.ichecklibs.view.appbackground.white_background.ICLinearLayoutWhite>