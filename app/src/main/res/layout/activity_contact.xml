<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="vn.icheck.android.screen.user.contact.ContactActivity">

    <include
        android:id="@+id/header"
        layout="@layout/toolbar_blue_v2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/size_10"
        android:background="@android:color/white"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/header">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/size_40"
            android:layout_marginTop="@dimen/size_8">
            <vn.icheck.android.ichecklibs.view.second_text.TextSecondBarlowMedium
                android:id="@+id/textView67"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/size_12"
                android:layout_weight="4"
                android:drawableStart="@drawable/ic_contact_22dp"
                android:drawableLeft="@drawable/ic_contact_22dp"
                android:drawablePadding="@dimen/size_6"
                android:gravity="center_vertical"
                android:text="@string/cham_soc_khac_hang"
                />
            <vn.icheck.android.ichecklibs.view.primary.TextBarlowMediumPrimary
                android:id="@+id/phone"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/size_8"
                android:layout_marginEnd="@dimen/size_12"
                android:layout_weight="6"
                android:gravity="end"
                tools:text="0902 195 488" />
        </LinearLayout>
        <View
            android:id="@+id/divider11"
            android:layout_width="match_parent"
            android:layout_height="@dimen/size_1"
            android:layout_marginStart="@dimen/size_12"
            android:layout_marginLeft="@dimen/size_12"
            android:layout_marginEnd="@dimen/size_12"
            android:layout_marginRight="@dimen/size_12"
            android:background="?android:attr/listDivider"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/phone"
            android:layout_marginBottom="@dimen/size_8"
            android:layout_marginTop="@dimen/size_8"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/size_40">

            <vn.icheck.android.ichecklibs.view.second_text.TextSecondBarlowMedium
                android:id="@+id/textView74"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/size_12"
                android:layout_marginLeft="@dimen/size_12"
                android:layout_weight="4"
                android:drawableStart="@drawable/ic_contact_mail_22dp"
                android:drawableLeft="@drawable/ic_contact_mail_22dp"
                android:drawablePadding="@dimen/size_6"
                android:gravity="center_vertical"
                android:text="@string/email" />
            <vn.icheck.android.ichecklibs.view.primary.TextBarlowMediumPrimary
                android:id="@+id/email"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/size_8"
                android:layout_marginEnd="@dimen/size_12"
                android:layout_weight="6"
                android:gravity="end"
                app:layout_constraintBaseline_toBaselineOf="@+id/textView74"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/guideline2"
                tools:text="cskh@icheck.vn" />
        </LinearLayout>
        <View
            android:id="@+id/divider17"
            android:layout_width="match_parent"
            android:layout_height="@dimen/size_1"
            android:layout_marginStart="@dimen/size_12"
            android:layout_marginLeft="@dimen/size_12"
            android:layout_marginEnd="@dimen/size_12"
            android:layout_marginRight="@dimen/size_12"
            android:layout_marginBottom="@dimen/size_8"
            android:background="?android:attr/listDivider"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/email"
            android:layout_marginTop="@dimen/size_8"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/size_40"
            android:layout_marginBottom="8dp"
            >
            <vn.icheck.android.ichecklibs.view.second_text.TextSecondBarlowMedium
                android:id="@+id/textView79"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/size_12"
                android:layout_marginLeft="@dimen/size_12"
                android:layout_weight="4"
                android:drawableStart="@drawable/ic_contact_facebook_22dp"
                android:drawableLeft="@drawable/ic_contact_facebook_22dp"
                android:drawablePadding="@dimen/size_6"
                android:gravity="center_vertical"
                android:text="@string/facebook"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider17" />
            <vn.icheck.android.ichecklibs.view.primary.TextBarlowMediumPrimary
                android:id="@+id/fb"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/size_8"
                android:layout_marginEnd="@dimen/size_12"
                android:layout_weight="6"
                android:gravity="end"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/guideline2"
                app:layout_constraintTop_toTopOf="@+id/textView79"
                tools:text="facebook.com/iCheck.com.vn" />
        </LinearLayout>
    </androidx.appcompat.widget.LinearLayoutCompat>

</androidx.constraintlayout.widget.ConstraintLayout>