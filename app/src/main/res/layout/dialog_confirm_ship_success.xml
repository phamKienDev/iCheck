<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    tools:background="@color/black_50"
    android:layout_gravity="center"
    android:background="@android:color/transparent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_24"
        android:layout_marginEnd="@dimen/size_24"
        android:background="@drawable/bg_corner_white_10"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <vn.icheck.android.ichecklibs.view.normal_text.TextHeaderNormalTextColor
            android:id="@+id/textView89"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/size_72"
            android:gravity="center"
            android:text="Tiếp nhận thông tin thành công"/>

        <TextView
            android:id="@+id/textView90"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/size_18"
            android:layout_marginTop="@dimen/size_10"
            android:layout_marginEnd="@dimen/size_18"
            android:fontFamily="@font/barlow_medium"
            android:gravity="center"
            android:text="Đơn hàng của bạn sẽ được iCheck giao tới bạn trong 2-4 ngày tới" />

        <TextView
            android:id="@+id/btn_observe"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/size_32"
            android:layout_marginTop="@dimen/size_16"
            android:layout_marginEnd="@dimen/size_32"
            android:layout_marginBottom="@dimen/size_16"
            android:background="@drawable/background_button_enable"
            android:fontFamily="@font/barlow_medium"
            android:gravity="center"
            android:padding="@dimen/size_8"
            android:text="Theo dõi đơn hàng"
            android:textColor="@android:color/white"
            android:textSize="@dimen/font_16" />
    </LinearLayout>

    <de.hdodenhof.circleimageview.CircleImageView
        android:layout_width="@dimen/size_110"
        android:layout_height="@dimen/size_110"
        android:src="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout9"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout9"
        app:layout_constraintStart_toStartOf="@+id/linearLayout9"
        app:layout_constraintTop_toTopOf="@+id/linearLayout9" />

    <ImageView
        android:id="@+id/circleImageView2"
        android:layout_width="@dimen/size_90"
        android:layout_height="@dimen/size_90"
        android:src="@drawable/ic_confirm_success"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout9" />

    <ImageView
        android:id="@+id/btn_cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/size_20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout9"
        app:srcCompat="@drawable/ic_cancel_popup_32px" />
</androidx.constraintlayout.widget.ConstraintLayout>