<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_error_request" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/size_16"
        android:fontFamily="sans-serif"
        android:text="@string/khong_the_truy_cap_vui_long_thu_lai_sau"
        android:textColor="#828282"
        android:textSize="@dimen/font_14"
        android:textStyle="normal" />

    <vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICTextSecondaryBgWhiteStrokeBlue1
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_32"
        android:layout_marginEnd="@dimen/size_32"
        android:fontFamily="sans-serif-medium"
        android:padding="@dimen/size_16"
        android:text="@string/trang_chu"
        android:textAlignment="center"
        android:textSize="@dimen/font_16"
        android:textStyle="normal" />
</LinearLayout>