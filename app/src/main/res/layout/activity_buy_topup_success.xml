<?xml version="1.0" encoding="utf-8"?>
<vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICLinearLayoutWhite xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="vn.icheck.android.screen.user.payment_topup_success.BuyTopupSuccessActivity">

    <vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICConstraintLayoutWhite
        android:id="@id/layoutToolbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_44"
        android:elevation="@dimen/size_4">

        <vn.icheck.android.ichecklibs.view.primary.ImageButtonPrimary
            android:id="@id/imgBack"
            android:layout_width="@dimen/size_44"
            android:layout_height="0dp"
            android:background="?android:attr/selectableItemBackground"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_back_blue_v2_24px" />

        <vn.icheck.android.ichecklibs.view.primary.TextHeaderPrimary
            android:id="@id/txtTitle"
            android:layout_width="match_parent"
            android:layout_height="@dimen/size_44"
            android:ellipsize="end"
            android:gravity="center"
            android:text="@string/giao_dich_thanh_cong"
            android:textStyle="normal"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/imgHistoryTopup"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/size_0"
            android:layout_gravity="end"
            android:background="?android:attr/selectableItemBackground"
            android:padding="@dimen/size_6"
            android:src="@drawable/ic_history_24px"
            app:layout_constraintBottom_toBottomOf="@id/txtTitle"
            app:layout_constraintEnd_toStartOf="@id/imgHome"
            app:layout_constraintTop_toTopOf="@id/txtTitle" />

        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/imgHome"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/size_0"
            android:layout_gravity="end"
            android:background="?android:attr/selectableItemBackground"
            android:padding="@dimen/size_6"
            android:src="@drawable/ic_home_blue_v2_24px"
            app:layout_constraintBottom_toBottomOf="@id/txtTitle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@id/txtTitle" />
    </vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICConstraintLayoutWhite>

    <vn.icheck.android.ichecklibs.view.normal_text.TextNormalBarlowMedium
        android:id="@+id/tvMessageSuccess"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_37"
        android:layout_marginTop="@dimen/size_10"
        android:layout_marginEnd="@dimen/size_37"
        android:gravity="center"
        android:lineSpacingExtra="@dimen/font_5_6"
        android:textAlignment="center"
        android:textSize="@dimen/font_14"
        android:textStyle="normal"
        tools:text="Quý khách đã nạp thành công \n 1 mã thẻ điện thoại Mobifone" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@id/view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_3"
        android:layout_marginStart="@dimen/size_13"
        android:layout_marginTop="@dimen/size_10"
        android:layout_marginEnd="@dimen/size_13"
        android:layerType="software"
        android:src="@drawable/line_dotted" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_12"
        android:layout_marginTop="@dimen/size_19"
        android:layout_marginEnd="@dimen/size_12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv1">

        <vn.icheck.android.ichecklibs.view.second_text.TextSecondBarlowMedium
            android:id="@+id/appCompatTextView28"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/nha_mang_colon"
            android:textSize="@dimen/font_14"
            app:layout_constraintEnd_toStartOf="@+id/tvName"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <vn.icheck.android.ichecklibs.view.normal_text.TextNormalBarlowMedium
            android:id="@+id/tvName"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:textSize="@dimen/font_14"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/appCompatTextView28"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Mobifone" />

        <vn.icheck.android.ichecklibs.view.second_text.TextSecondBarlowMedium
            android:id="@+id/appCompatTextView3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/size_10"
            android:text="@string/nap_cho_so_colon"
            android:textSize="@dimen/font_14"
            app:layout_constraintEnd_toStartOf="@+id/tvPhone"
            app:layout_constraintStart_toStartOf="@+id/appCompatTextView28"
            app:layout_constraintTop_toBottomOf="@+id/appCompatTextView28" />

        <vn.icheck.android.ichecklibs.view.normal_text.TextNormalBarlowMedium
            android:id="@+id/tvPhone"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/size_10"
            android:gravity="end"
            android:textSize="@dimen/font_14"
            app:layout_constraintEnd_toEndOf="@+id/tvName"
            app:layout_constraintStart_toEndOf="@+id/appCompatTextView3"
            app:layout_constraintTop_toBottomOf="@+id/tvName"
            tools:text="0987 225 214" />

        <vn.icheck.android.ichecklibs.view.second_text.TextSecondBarlowMedium
            android:id="@+id/appCompatTextView29"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/size_10"
            android:text="@string/tong_thanh_toan_colon"
            android:textSize="@dimen/font_14"
            app:layout_constraintEnd_toStartOf="@+id/tvTotal"
            app:layout_constraintStart_toStartOf="@+id/appCompatTextView3"
            app:layout_constraintTop_toBottomOf="@+id/appCompatTextView3" />

        <vn.icheck.android.ichecklibs.view.normal_text.TextNormalBarlowMedium
            android:id="@+id/tvTotal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/size_10"
            android:gravity="end"
            android:textSize="@dimen/font_14"
            app:layout_constraintEnd_toEndOf="@+id/tvPhone"
            app:layout_constraintStart_toEndOf="@+id/appCompatTextView29"
            app:layout_constraintTop_toBottomOf="@+id/tvPhone"
            tools:text="100,000đ" />

        <vn.icheck.android.ichecklibs.view.second_text.TextSecondBarlowMedium
            android:id="@+id/appCompatTextView31"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/size_10"
            android:text="@string/hinh_thuc_thanh_toan_colon"
            android:textSize="@dimen/font_14"
            app:layout_constraintEnd_toStartOf="@+id/tvTypePayment"
            app:layout_constraintStart_toStartOf="@+id/appCompatTextView29"
            app:layout_constraintTop_toBottomOf="@+id/appCompatTextView29" />

        <vn.icheck.android.ichecklibs.view.normal_text.TextNormalBarlowMedium
            android:id="@+id/tvTypePayment"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/size_10"
            android:gravity="end"
            android:textSize="@dimen/font_14"
            app:layout_constraintEnd_toEndOf="@+id/tvTotal"
            app:layout_constraintStart_toEndOf="@+id/appCompatTextView31"
            app:layout_constraintTop_toBottomOf="@+id/tvTotal" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <vn.icheck.android.ichecklibs.view.primary.TextBarlowSemiBoldPrimary
            android:id="@+id/btnTopupAgain"
            android:layout_width="match_parent"
            android:layout_height="@dimen/size_36"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/size_37"
            android:layout_marginEnd="@dimen/size_38"
            android:gravity="center"
            android:text="@string/nap_the_dien_thoai_khac"
            android:textSize="@dimen/font_16"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </LinearLayout>
</vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICLinearLayoutWhite>