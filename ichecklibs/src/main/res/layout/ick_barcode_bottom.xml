<?xml version="1.0" encoding="utf-8"?>
<vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICConstraintLayoutBgWhiteRadiusTop16 xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <vn.icheck.android.ichecklibs.view.primary.ImageButtonPrimary
        android:id="@+id/btn_clear"
        android:layout_width="@dimen/size_50"
        android:layout_height="@dimen/size_50"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/barrier3"
        app:layout_constraintBottom_toBottomOf="@id/barrier2"
        app:srcCompat="@drawable/ic_cancel_blue_24px" />

    <vn.icheck.android.ichecklibs.view.primary.ImageButtonPrimary
        android:id="@+id/imgHohder"
        android:layout_width="@dimen/size_50"
        android:layout_height="@dimen/size_50"
        android:background="@android:color/transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/barrier3"
        app:layout_constraintBottom_toBottomOf="@id/barrier2"/>

    <vn.icheck.android.ichecklibs.view.primary.TextBarlowSemiBoldPrimary
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/nhap_ma_bang_tay"
        android:gravity="center"
        android:textSize="@dimen/font_18"
        app:layout_constraintBottom_toBottomOf="@id/barrier3"
        app:layout_constraintEnd_toStartOf="@+id/imgHohder"
        app:layout_constraintStart_toEndOf="@+id/btn_clear"
        app:layout_constraintTop_toTopOf="@id/barrier2" />

    <vn.icheck.android.ichecklibs.view.line_color.ICViewLineColor
        android:id="@+id/view9"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/size_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/barrier3" />

    <vn.icheck.android.ichecklibs.view.normal_text.FocusableEdittextNormal
        android:id="@+id/edtBarcode"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_16"
        android:layout_marginLeft="@dimen/size_16"
        android:layout_marginTop="@dimen/size_32"
        android:layout_marginEnd="@dimen/size_16"
        android:layout_marginRight="@dimen/size_16"
        android:fontFamily="@font/barlow_medium"
        android:gravity="center"
        android:hint="@string/nhap_ma_san_pham"
        android:inputType="number"
        android:ellipsize="end"
        android:paddingStart="@dimen/size_32"
        android:paddingEnd="@dimen/size_32"
        android:paddingBottom="@dimen/size_20"
        android:textSize="@dimen/font_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view9" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/btnKeyboard"
        android:layout_width="@dimen/size_24"
        android:layout_height="@dimen/size_24"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_keyboard_gray_18dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@id/edtBarcode"
        app:layout_constraintEnd_toEndOf="@id/edtBarcode"
        app:layout_constraintTop_toTopOf="@id/edtBarcode" />

    <vn.icheck.android.ichecklibs.view.second_text.TextSecondBarlowMedium
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/size_8"
        android:text="@string/nhap_ma_duoc_in_tren_san_pham"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtBarcode" />

    <vn.icheck.android.ichecklibs.EnableButton
        android:id="@+id/submit_barcode"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/size_36"
        android:layout_marginStart="@dimen/size_16"
        android:layout_marginTop="@dimen/size_8"
        android:layout_marginEnd="@dimen/size_16"
        android:layout_marginBottom="@dimen/size_16"
        android:fontFamily="@font/barlow_medium"
        android:text="@string/check_ma"
        android:textAllCaps="false"
        android:textColor="@android:color/white"
        android:textSize="@dimen/font_16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView29" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="top"
        app:constraint_referenced_ids="imgHohder, btn_clear, textView" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="textView,btn_clear,imgHohder" />
</vn.icheck.android.ichecklibs.view.appbackground.whitebackground.ICConstraintLayoutBgWhiteRadiusTop16>